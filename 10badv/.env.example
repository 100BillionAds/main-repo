# Example environment variables for local development
# Copy to .env.local and fill values as needed

# Public API base (used in client code)
NEXT_PUBLIC_API_URL=http://localhost:3000/api

# Database (if used)
# For Postgres: postgres://user:pass@localhost:5432/dbname
DATABASE_URL=

# Authentication
NEXTAUTH_SECRET=
NEXTAUTH_URL=http://localhost:3000

# Optional: Sentry, third-party API keys
SENTRY_DSN=
THIRD_PARTY_API_KEY=
